ll add(ll a, ll b) {
	return (a+b)%MOD;
}
ll sub(ll a, ll b) {
	return (a-b+MOD)%MOD;
}
ll mult(ll a, ll b) {
	return ((long long) a*b)%MOD;
}
]]></content>
